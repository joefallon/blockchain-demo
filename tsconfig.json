{
  "compilerOptions": {
    "alwaysStrict": true,
    "downlevelIteration": true,
    "inlineSourceMap": true,
    "jsx": "react",
    "importHelpers": true,
    "module": "commonjs",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "skipLibCheck": true,
    "strictFunctionTypes": true,
    "target": "es5"
  },
  "include": [
    "typings/**/*",
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}